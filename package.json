{
  "name": "angular2-electron-quickstart",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "npm run electron",
    "build": "tsc",
    "web": "tsc && concurrently \"npm run tsc:w\" \"gulp\" \"npm run lite\" ",
    "electron": "tsc && gulp scss && electron main.js",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings"
  },
  "keywords": [
    "angular2",
    "Electron",
    "quick",
    "start",
    "tutorial"
  ],
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "systemjs": "0.19.27",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12",
    "angular2-in-memory-web-api": "0.0.11",
    "bootstrap": "^3.3.6"
  },
  "devDependencies": {
    "electron-prebuilt": "^1.2.0",
    "jspm": "^0.15.7",
    "concurrently": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-scss": "^1.3.17",
    "gulp-watch": "^4.3.6",
    "lite-server": "^2.2.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
}
